# Example environment variables for ClipOpera AI Ad Studio

# AWS S3
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-key
S3_BUCKET_NAME=your-bucket-name
AWS_REGION=us-west-1

# Meta Ads
META_APP_ID=your-meta-app-id
META_APP_SECRET=your-meta-secret
META_REDIRECT_URI=https://yourdomain.com/meta/callback

PLACEHOLDER_MODEL_VIDEO_URL=https://your-s3-url/placeholder_model.mp4
MOCK_RENDERED_3D_VIDEO_URL=https://your-s3-url/mock_rendered_3d.mp4
ANIME_10S_PLACEHOLDER_URL=https://your-s3-url/anime_10s.mp4
ANIME_30S_PLACEHOLDER_URL=https://your-s3-url/anime_30s.mp4
ANIME_60S_PLACEHOLDER_URL=https://your-s3-url/anime_60s.mp4
ASCII_AD_PLACEHOLDER_URL=https://your-s3-url/ascii.mp4
RETRO_8BIT_AD_PLACEHOLDER_URL=https://your-s3-url/8bit.mp4
RETRO_16BIT_AD_PLACEHOLDER_URL=https://your-s3-url/16bit.mp4
RETRO_32BIT_AD_PLACEHOLDER_URL=https://your-s3-url/32bit.mp4

# Celery
CELERY_BROKER_URL=redis://localhost:6379/0

# AI Keys
GEMINI_API_KEY=your-gemini-api-key
OPENAI_API_KEY=your-openai-api-key

# FastAPI Auth Demo Credentials
SECRET_KEY=change_me
DEMO_USERNAME=demo
DEMO_PASSWORD=password
